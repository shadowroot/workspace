package socket;

import java.net.InetAddress;


public class table {
	private String[] data;
	private int i=0;
	private InetAddress[] host;
	private int[] port;
	public void update(String dat,InetAddress hos,int por) {
		data[i] = dat;
		host[i] = hos;
		port[i] = por;
		i++;
	}
	
	public int getI(){
		return i;
	}
	public String getData(){
		
	}
	
	
}
