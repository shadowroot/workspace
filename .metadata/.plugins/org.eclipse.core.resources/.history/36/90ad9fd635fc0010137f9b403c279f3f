<?php
/*
 * CHACHA CUBE
 * 
 * Compression algorithm using cube like bigger 3D sudoku.
 * Using checksums in each square
 * 3sqrt of size in Bytes = resolution of square
 * 
 */
$filename = 'test.dat';
$file = fopen($filename, 'a+');
$size = filesize($filename);
$data = fread($file, $size);
fclose($file);

$row = pow($size, (1/3));

$index = 0;
$sum_x = array();
$sum_y = array();
$sum_z = array();

for ($z=0;$z<=$row;$z++){
	for($y=0;$y<=$row;$y++){
		$suma = 0;
		for($x=0;$x<=$row;$x++){
			$suma += $data[$index];
			$sume_z[$x] += $data[$index];
			$index++;
		}
		$sum_x[$y] = $suma;
	}
	
}

?>