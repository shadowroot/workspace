document.prototype.onkeydown = function(e){
			switch(e.keyCode){
				case 113/*q*/:
					this.hero.multipler++;
					break;
				case 119/*w*/:
					this.hero.multipler--;
					break;
				case 102/*f*/:
					
					break;
				case 103/*g*/:
					
					break;
				default:
					break;
			}

};
