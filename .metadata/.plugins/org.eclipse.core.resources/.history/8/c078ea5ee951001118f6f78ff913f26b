<script type="text/javascript">
function game(){
		
	this.const = 5;/*18*/
	this.hero = {
			/*
				Start coordinates
			*/
		x:50,
		y:50,
		points:new Array(),
		//[x,y] vector 
		direction:[1,0],
		multipler:1
	};
	var player1 = {
		x:0,
		y:0,
		bez_1:0,
		bez_2:0
	};
	var player2 = {
			
	};
	var player3 = {
			
	};
	var player4 = {
			
	};
	hero.points.push([22,33]);
	this.canvas = document.getElementById("gamespace");
	this.ctx = this.canvas.getContext("2d");
	ctx.beginPath();
	ctx.moveTo(100,100);
	ctx.lineTo(101,101);
	ctx.lineTo(102,102);
	ctx.lineWidth = 10;
	ctx.strokeStyle = "black";
	ctx.stroke();
	ctx.fillStyle="#000000";
	ctx.fillRect(0,0,300,300);
	var data = ctx.getImageData(0,0,300,300);
	var i=0;
	
	for (i=0;i<=5;i++){
		//90% alfa
		if((data.data[i] | 0x00) != 0x00){
			document.write("collision");
		}
		
	}
	this.init = function(){
		
	};
	this.draw = function(){
		this.ctx.beginPath();
		this.ctx.moveTo(this.hero.x,this.hero.y);
		
	};
	this.change_vector = function(){

		var x = Math.sin(this.const*this.hero.multipler);
		
		var y = Math.sqrt(1-x*x);

		this.hero.direction[x,y];
	};
}
/*
 * Constructor
 */
var game = new game();
game.init();
