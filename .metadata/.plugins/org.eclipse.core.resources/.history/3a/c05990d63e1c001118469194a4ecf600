if($_SESSION['privilege'] == 0){
	echo '<table><tr><td>ID</td><td>USER</td><td>PRIVILEGE</td><td>AUTHORIZED</td><td>E-mail</td></tr>';
	$q = mysql_query("select * from users");
		while($row = mysql_fetch_array($q)){
